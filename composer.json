{
    "name": "mindtwo/px-mail-laravel-driver",
    "type": "proprietary",
    "description": "A project to integrate TX Mail as mail service in Laravel.",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://mindtwo.de"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "larastan/larastan": "^2.0||^3.0",
        "orchestra/testbench": "^8.21||^9.0||^10.0||^11.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mindtwo\\LaravelPxMail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\LaravelPxMail\\Providers\\LaravelPxMailProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
